<%= form_for [@group, @student] do |f| %>
    <div class="form-group">
      <label>Full name</label>
      <%= f.text_field :full_name %>
    </div>
    <div class="form-group">
      <label>Image link</label>
      <%= f.text_field :image %>
    </div>
    <div class="form-group">
      <label>Batch</label>
      <%= f.number_field :batch %>
    </div>
    <%= f.submit 'Save' %>
  <%= link_to "Back", group_path(@group.id) %>
<% end %>
